{%  extends '::base.html.twig' %}
{% block content %}
    <div class="container">
        <div class="row">
            <h1>{{ page.title }}</h1>
            <div class="col-sm-12 col-md-12">
                {{ page.content|raw }}
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-12">
                <p class="text-right">{{ page.createdat |date('Y-M-D') }}</p>
            </div>
        </div>
    </div>
{% endblock %}
